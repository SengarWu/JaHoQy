<?xml version="1.0" encoding="UTF-8"?>
<com.xpple.jahoqy.addimages.SquareLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <ImageView
        android:id="@+id/imageview_item_one"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:src="@drawable/choice_2_1"
        android:visibility="gone" />

    <FrameLayout
        android:id="@+id/framelayout"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:descendantFocusability="blocksDescendants" >

        <com.xpple.jahoqy.addimages.LocalImageView
            android:id="@+id/child_image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop" />
        <View 
            android:id="@+id/view_ImageView_up"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#40000000"
            android:visibility="gone"
            ></View>

 <!-- android:layout_width="@dimen/photo_history_detail_container_heigth"
            android:layout_height="@dimen/photo_history_detail_container_heigth" -->
        <ImageView
            android:id="@+id/child_checkbox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_margin="8dp"
            android:background="@drawable/pictures_select_icon"
            android:button="@null" />

        <!-- android:button="@drawable/pictures_select_icon" -->
    </FrameLayout>
</com.xpple.jahoqy.addimages.SquareLayout>